a = int(input("enter number to check is it prime or not\n"))
n = int(a/2)
print("following are the divider of ", a)
count =0
for i in range(2,n+1):
    if a % i == 0:
        count += 1
        print(i)
    # break
print("\t")
if count > 0:
    print("so,",a, "is not prime number")
else:
    print(a, "is prime number")
